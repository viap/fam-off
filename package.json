  "main": "index.js",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "antd": "^5.0.0",
    "classnames": "^2.3.2",
    "mongodb": "^4.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "postcss-d-ts": "^1.2.0",
